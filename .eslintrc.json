{
  "env": {
    "node": true,
    "es6": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": { 
    "ecmaVersion": "2018"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-undef": "error",
    "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
  },
  "extends": ["prettier"],
  "root": true
}
